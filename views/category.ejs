<%- include('partials/header') %>

        <div class="page-title lb single-wrapper">
            <div class="container">
                <div class="row">
                    <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                        <h2><i class="fa fa-gears bg-orange"></i> FOOTBALL <small class="hidden-xs-down hidden-sm-down">Cùng xem giải đấu của bạn theo dõi có gì thú vị nào. </small></h2>
                    </div><!-- end col -->
                    <div class="col-lg-4 col-md-4 col-sm-12 hidden-xs-down hidden-sm-down">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="#">Trang chủ</a></li>
                            <li class="breadcrumb-item"><a href="#">Bìa viết</a></li>
                            <li class="breadcrumb-item active">Giải đấu</li>
                        </ol>
                    </div><!-- end col -->                    
                </div><!-- end row -->
            </div><!-- end container -->
        </div><!-- end page-title -->

        <section class="section">
            <div class="container">
                <div class="row">
                    <%- include('partials/side') %>
                    <div class="col-lg-9 col-md-12 col-sm-12 col-xs-12">
                        <div class="page-wrapper">
                            <div class="blog-grid-system">
                                <div class="row">
                                    <% cate.forEach(function(cate){ %>
                                        <div class="col-md-6">
                                            <div class="blog-box">
                                                <div class="post-media">
                                                    <a href="/detail/<%= cate.id %>" title="">
                                                        <img src="../upload/<%= cate.image %>" alt="" class="img-fluid">
                                                        <div class="hovereffect">
                                                            <span></span>
                                                        </div><!-- end hover -->
                                                    </a>
                                                </div><!-- end media -->
                                                <div class="blog-meta big-meta">
                                                    <span class="color-orange"><a href="tech-category-01.html" title="">Gadgets</a></span>
                                                    <h4><a href="/detail/<%= cate.id %>" title=""><%= cate.name %></a></h4>
                                                    <p style="overflow: hidden;text-overflow: ellipsis;line-height: 20px;-webkit-line-clamp: 5;display: -webkit-box;-webkit-box-orient: vertical;text-align: justify;"><%= cate.summary %>.</p>
                                                    <small><a href="/detail/<%= cate.id %>" title=""><%= cate.created_at %></a></small>
                                                    <small><a href="/detail/<%= cate.id %>" title="">by <%= cate.author %></a></small>
                                                    <small><a href="/detail/<%= cate.id %>" title=""><i class="fa fa-eye"></i> 2887</a></small>
                                                </div><!-- end meta -->
                                            </div><!-- end blog-box -->
                                        </div><!-- end col -->
                                    <% }); %>
                                </div><!-- end row -->
                            </div><!-- end blog-grid-system -->
                        </div><!-- end page-wrapper -->

                        <hr class="invis3">

                        <div class="row">
                            <div class="col-md-12">
                                <nav aria-label="Page navigation">
                                    <ul class="pagination justify-content-start">
                                        <li class="page-item"><a class="page-link" href="#">1</a></li>
                                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                                        <li class="page-item">
                                            <a class="page-link" href="#">Next</a>
                                        </li>
                                    </ul>
                                </nav>
                            </div><!-- end col -->
                        </div><!-- end row -->
                    </div><!-- end col -->
                </div><!-- end row -->
            </div><!-- end container -->
        </section>

<%- include('partials/footer') %>
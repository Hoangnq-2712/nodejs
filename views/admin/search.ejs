<%- include('partial/ad_header') %>
            <%- include('partial/sidebar') %>
        <div class="content-wrapper">
            <!-- <h2><%= title %></h2> -->
            <div class="container">
            <nav class="navbar navbar-light ">
              <div class="container-fluid">
                <!-- <a class="navbar-brand">Navbar</a> -->
                <h2><%= title %></h2>
                <form class="d-flex" action="/admin/search" method="post">
                  <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search" name="search">
                  <button class="btn btn-outline-success" type="submit">Search</button>
                </form>
              </div>
            </nav>

            <table class="table table-dark table-striped">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>category_id</th>
                  <th>user_id</th>
                  <th>image</th>
                  <th>DESCRIPTION</th>
                  <th>Action</th>
                </tr>
              </thead>
              <tbody>
                    <% search.forEach(function(search){ %>
                      <tr>
                          <td><%= search.id %></td>
                          <td><%= search.name %></td>
                          <td><%= search.category_id %></td>
                          <td><%= search.user_id %></td>
                          <td><img src="/upload/<%= search.image %>" width="50" height="50"></td>
                          <td style="text-align:justify;"><%= search.summary %></td>
                          <td>
                              <a href="post/edit/<%= search.id %>" class="btn btn-sm btn-primary">Edit</a>
                              <a href="post/delete/<%= search.id %>" class="btn btn-sm btn-danger" >Delete</a>
                          </td>
                      </tr>
                    <%}); %>
              </tbody>
            </table>
          </div>
          </div>
<%- include('partial/ad_footer') %>